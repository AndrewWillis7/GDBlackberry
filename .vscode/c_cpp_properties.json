{
  "configurations": [
    {
      "name": "Windows (GDExtension)",
      "includePath": [
        "${workspaceFolder}/godot-cpp/include/**",
        "${workspaceFolder}/godot-cpp/gdextension/**",
        "${workspaceFolder}/godot-cpp/gen/**"
      ],
      "defines": [
        "GDEXTENSION_LIBRARY",
        "WINDOWS_ENABLED",
        "TYPED_METHOD_BIND"
      ],
      "cStandard": "c17",
      "cppStandard": "c++17",
      "intelliSenseMode": "windows-gcc-x64",
      "browse": {
        "path": [
          "${workspaceFolder}"
        ],
        "limitSymbolsToIncludedHeaders": true
      }
    },

   {
      "name": "Windows",
      "includePath": [
        "${workspaceFolder}/godot-cpp/include",
        "${workspaceFolder}/godot-cpp/gdextension",
        "${workspaceFolder}/godot-cpp/include/core",
        "${workspaceFolder}/godot-cpp/include/gen"
      ],
      "defines": ["GDEXTENSION_LIBRARY"]
    }
  ],
  "version": 4
}